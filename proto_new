syntax = "proto3";

package amakna;

message MapComplementaryInformationEvent {
    uint32 subareaId = 1; // Renomeado de subAreaId para subareaId
    int64 mapId = 2; // Tipo alterado para int64
    repeated House houses = 3; // Renomeado de HouseInformations para House
    repeated ActorPositionInformation actors = 4; // Renomeado de GameRolePlayActorInformations para ActorPositionInformation
    repeated InteractiveElement interactiveElements = 5;
    repeated StatedElement statedElements = 6;
    repeated MapObstacle obstacles = 7;
    repeated FightCommonInformation fights = 8; // Renomeado de FightCommonInformations para FightCommonInformation
    bool hasAggressiveMonsters = 9;
    FightStartingPositions fightStartPositions = 10;
    oneof specificComplementaryInformation {
        MapComplementaryInHouseInformation inHouseInformation = 11;
        MapCoordinates coordinates = 12;
        MapComplementaryBreachInformation breachInformation = 13;
        MapComplementaryAnomalyInformation anomalyInformation = 14;
        MapComplementaryHavenBagInformation havenBagInformation = 15;
    }
}


message House {
    uint32 houseId = 1;
    uint32 modelId = 2;
}


message ActorPositionInformation {
    double contextualId = 1;
    EntityDispositionInformations disposition = 2;
    EntityLook look = 3;
}